


<div class="row">
  <div class="col-sm-6">
    <h1>Feedbacks</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_feedback_path, class: 'btn btn-primary' do %>
    Add New Feedback
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Caller</th>
            <th>Report</th>
            <th>Audio url</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @feedbacks.each do |feedback| %>
        <%= content_tag :tr, id: dom_id(feedback), class: dom_class(feedback) do %>
          

                      <td><%= feedback.caller_id %></td>
                      <td><%= feedback.report_id %></td>
                      <td><%= feedback.audio_url %></td>
          
          
            <td><%= link_to 'Show', feedback %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
