


<div class="row">
  <div class="col-sm-6">
    <h1>Sectors</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_sector_path, class: 'btn btn-primary' do %>
    Add New Sector
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>

        <th>Name</th>


            <th>District</th>

      </tr>
    </thead>

    <tbody>
      <% @sectors.each do |sector| %>
        <%= content_tag :tr, id: dom_id(sector), class: dom_class(sector) do %>

            <td><%= link_to sector.name, sector %></td>

                      <% district = District.find(sector.district_id) %>
                      <td><%= link_to district.name, district  %></td>
                      <!-- <td><%= sector.district_id %></td> -->


        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
