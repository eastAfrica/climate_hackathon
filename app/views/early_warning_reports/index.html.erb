


<div class="row">
  <div class="col-sm-6">
    <h1>Early_warning_reports</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_early_warning_report_path, class: 'btn btn-primary' do %>
    Add New Early warning report
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Early warning</th>
            <th>Sector</th>
            <th>Level</th>
            <th>Feedback</th>
            <th>Reciever</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @early_warning_reports.each do |early_warning_report| %>
        <%= content_tag :tr, id: dom_id(early_warning_report), class: dom_class(early_warning_report) do %>
          

                      <td><%= early_warning_report.early_warning_id %></td>
                      <td><%= early_warning_report.sector_id %></td>
                      <td><%= early_warning_report.level %></td>
                      <td><%= early_warning_report.feedback_id %></td>
                      <td><%= early_warning_report.reciever_id %></td>
          
          
            <td><%= link_to 'Show', early_warning_report %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
